{
  "name": "ci-cd",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "Continuous Integration/Continuous Deployment environment and scripts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/aboe026/ci-cd.git"
  },
  "bugs": {
    "url": "https://github.com/aboe026/ci-cd/issues"
  },
  "homepage": "https://github.com/aboe026/ci-cd",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.4"
  },
  "scripts": {
    "backup": "ts-node src/backup.ts",
    "build": "cd compose && docker-compose build",
    "create": "cd compose && docker-compose up -d",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "start": "cd compose && docker-compose start",
    "stop": "cd compose && docker-compose stop"
  },
  "dependencies": {
    "archiver": "5.3.0",
    "bytes": "3.1.0",
    "cli-progress": "3.9.0",
    "dockerode": "3.3.0",
    "dotenv": "9.0.0",
    "envalid": "7.1.0",
    "fast-folder-size": "1.3.0",
    "fs-extra": "10.0.0",
    "humanize-duration": "3.26.0",
    "log4js": "6.3.0",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@types/archiver": "5.1.0",
    "@types/bytes": "3.1.0",
    "@types/cli-progress": "3.9.1",
    "@types/dockerode": "3.2.3",
    "@types/dotenv": "8.2.0",
    "@types/fs-extra": "9.0.11",
    "@types/humanize-duration": "3.18.1",
    "@types/node": "15.0.2",
    "@types/yargs": "16.0.1",
    "@typescript-eslint/parser": "4.22.1",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
